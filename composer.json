{
    "name": "nonsapiens/laravel-app-init",
    "description": "A migrations-like application initialiser, designed to allow commands and configurations to be run post-deploy",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nonsapiens\\LaravelAppInit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Stuart Steedman"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Nonsapiens\\LaravelAppInit\\Providers\\AppInitServiceProvider"
            ]
        }
    },
    "require": {
        "php": "8.*",
        "laravel/framework": "11.*|12.*"
    },
    "minimum-stability": "dev"
}